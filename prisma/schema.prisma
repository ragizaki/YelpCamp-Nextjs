// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Camp {
  id Int @id @default(autoincrement())
  name String
  image String
  price Int @default(0)
  city String
  state String
  description String
}
